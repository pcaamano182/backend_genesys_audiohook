{
  "name": "flex-template-agent-assist",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "test:watch": "jest --watch",
    "test:mychanges": "jest --onlyChanged",
    "test": "jest",
    "test:ci": "jest --silent --ci --coverage --coverageReporters=cobertura --reporters=default --reporters=jest-junit --testResultsProcessor=jest-junit",
    "test:coverage": "jest --silent --coverage && http-server -c-1 ./coverage -o /lcov-report",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "lint:report": "npm run lint -- --output-file eslint_report.json --format json",
    "build": "twilio flex:plugins:build",
    "deploy": "twilio flex:plugins:deploy --bypass-validation",
    "install-flex-plugin": "twilio plugins:install @twilio-labs/plugin-flex@7.0.5",
    "release": "twilio flex:plugins:release --plugin ${npm_package_name}@${npm_package_version}",
    "start": "twilio flex:plugins:start",
    "twilio": "twilio"
  },
  "dependencies": {
    "@emoji-mart/react": "^1.1.1",
    "@twilio-paste/core": "^17.0.1",
    "@twilio-paste/icons": "^9.4.2",
    "@twilio/flex-plugin-scripts": "7.0.5",
    "@types/loadjs": "^4.0.4",
    "emoji-mart": "^5.5.2",
    "js-cookie": "^3.0.5",
    "loadjs": "^4.3.0",
    "lodash": "^4.17.20",
    "luxon": "^3.1.1",
    "prop-types": "^15.7.2",
    "react": "17.0.2",
    "react-cookie": "^7.2.1",
    "react-dom": "17.0.2",
    "react-minimal-pie-chart": "^8.4.0",
    "rrule": "^2.7.1",
    "semver": "^7.6.3",
    "simple-audio-recorder": "^1.2.3",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.12.1",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^14.5.2",
    "@twilio/flex-ui": "^2.9.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^27.5.2",
    "@types/jest-axe": "^3.5.9",
    "@types/js-cookie": "^3.0.6",
    "@types/luxon": "^3.1.0",
    "@types/node": "^22.10.2",
    "@types/react-redux": "^7.1.1",
    "@types/semver": "^7.5.8",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.12.0",
    "eslint-config-twilio": "^2.0.0",
    "import-glob": "^1.5.0",
    "jest": "^27.5.1",
    "jest-axe": "^9.0.0",
    "jest-fetch-mock": "^3.0.3",
    "jest-junit": "^16.0.0",
    "react-test-renderer": "17.0.2",
    "typescript": "^5"
  },
  "jest-junit": {
    "outputDirectory": "./test-results"
  }
}
